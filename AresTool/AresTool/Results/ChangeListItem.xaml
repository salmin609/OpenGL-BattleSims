<UserControl x:Class="AresTool.Results.ChangeListItem"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:AresTool.Results"
             mc:Ignorable="d" 
             d:DesignHeight="30" d:DesignWidth="1200">
    <Grid>
        <TextBox x:Name="fileName" HorizontalAlignment="Left" TextWrapping="Wrap" Text="fileName" VerticalAlignment="Center" Height="20" FontSize="15" TextAlignment="Left" TextChanged="fileName_TextChanged" Width="970" Margin="24,0,0,0" Background="Transparent" BorderThickness="0,0,0,0"/>

    </Grid>
</UserControl>
