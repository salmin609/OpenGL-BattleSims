<UserControl x:Class="AresTool.Results.ImplementListItem"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:AresTool.Results"
             mc:Ignorable="d" 
             d:DesignHeight="20" d:DesignWidth="800">
    <Grid>
        <StackPanel x:Name="NamePanel" Orientation="Horizontal">
            <CheckBox x:Name="existCheckBox"  HorizontalAlignment="Left" Margin="5,0,0,0" VerticalAlignment="Center" Checked="existCheckBox_Checked" Unchecked="existCheckBox_Unchecked"/>
            <TextBlock x:Name="fileNameBlock" HorizontalAlignment="Left" Margin="26,0,0,0" TextWrapping="Wrap" VerticalAlignment="Center" Width="367" Text="File Name"/>
            <StackPanel Width="100"/>
        </StackPanel>
        
    </Grid>
</UserControl>
